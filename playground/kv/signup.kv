<Signup>:
    MDFloatLayout:
        md_bg_color: 1,1,1,1
    MDIconButton:
        icon: "arrow-left"
        pos_hint: {'center_x':.2, 'center_y':.8}
        on_release: 
            root.manager.current = 'welcome'
            root.manager.transition.direction = 'right'
    MDLabel:
        text: 'SignUp Screen'
        halign: 'center'
        pos_hint:{'center_y': .5}